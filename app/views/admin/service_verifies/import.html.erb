<% content_for :main_menu  do %>
  <%= render :partial => "admin/main/partials/menus/main", :locals => {:menu_type => "service_verify"} %>
<% end %>
<% content_for :sider_menu  do %>
  <%= render :partial => "admin/main/partials/menus/service_verify"%>
<% end %>

<div class="gm-bloc s-h-wrapper">
  <div class="gm-bloc-head">
    <h3 class="gm-bloc-title">
      导入序列号
    </h3>
    <a href="#" class="toggle"></a>
  </div>
  <div class="gm-bloc-body">
    <%= form_tag import_save_admin_service_verifies_path, :multipart => true do |f|%>
      <table>
        <tr>
          <td>
            分类：
          </td>
          <td>
            <%= select_tag "category_id", options_for_select(ServiceCategory.all.map{|category| [category.name, category.id]})%>
          </td>
        </tr>
        <tr>
          <td>
            文件：
          </td>
          <td>
            <%= file_field_tag :file %>
          </td>
        </tr>
        <tr>
          <td colspan="2">
            <%= submit_tag "提交" %>
          </td>
        </tr>
      </table>
    <%end%>
  </div>
</div>