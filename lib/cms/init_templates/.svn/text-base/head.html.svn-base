<script type="text/javascript">
 (function($){
			$(document).ready(function(){
				// This value can be true, false or a function to be used as a callback when the closer is clciked
				$.jGrowl("{{flash_error}}");
			});
		})(jQuery);
</script>
{% endif %}
 

 

