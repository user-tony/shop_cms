        <script type="text/javascript">
        var urls = {
            expand : 'images/toggle-small-expand.png',
            minus : 'images/toggle-small.png'
        };
    
        $('.s-sub-table tr.parent').each(function(index,ele){
            var tr = ele;
            $('img.open-sub',ele).click(function(){
                var ntr = $(ele).next('tr.child');
            
                if(ntr.hasClass('closed')){
                    ntr.removeClass('closed');
                    $(this).attr('src',urls.minus);
                }else{
                    ntr.addClass('closed');
                    $(this).attr('src',urls.expand);
                }
            });
        });
    </script>
    
