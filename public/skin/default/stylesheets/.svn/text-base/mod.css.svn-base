    margin: 0 10px;
    zoom: 1;
}
.gm-products-sets-scroll .gm-ilink-prev{
    float:left;
    padding: 10px;
    width: 102px;
}

.gm-products-sets-scroll .gm-ilink-prev .img{
    margin: 0px;
    display: block;
}
.gm-products-sets-scroll .gm-ilink-prev  p{
    clear: both;
}
.gm-products-sets-wrapper {
    position: relative;
    overflow: hidden;
    margin: 10px 40px;
    height: 195px;
}
.gm-products-sets-scroll .scroll-btn{
    position: absolute;
    display: block;
    width: 20px;
    height: 58px;
    top: 75px;
}
.gm-products-sets-scroll .left{
    left: 2px;
}
.gm-products-sets-scroll .right{
    right: 2px;
}

/* pros img list by */
