/*************comment*********************/

.gm-comment-list{
    overflow: hidden;
    clear: both;
}
.gm-comment-content{
    padding-left:101px;
    position:relative;
    margin-bottom:20px;
}
.gm-comment-content .user-info{
    position:absolute;
    top:0;
    left:0;
    _left:-101px;
    width:88px;
    font-size:14px;
    font-weight:bold;
    color:#5a5a5a;
    text-align:center;
}
.gm-comment-content .user-info img{
    width:84px;
    height:84px;
    padding:1px;
    border:1px solid #d5d5d5;
}
.gm-comment-content .comment{
    margin-bottom:15px;
    border:1px solid #dedfe1;
    border-radius:3px;
    position:relative;
    padding:13px;
    box-shadow:0px 6px 0px #f6f6f6;
}
.gm-comment-content .comment .title{
    font-size:14px;
    font-weight:bold;
    color:#83ab29;
    padding-bottom:5px;
}
.gm-comment-content .comment .question{
    padding:10px 0;
    border-top:1px dashed #e1e1e1;
    color:#5a5a5a;
    overflow:hidden;
}
.gm-comment-content .comment .answer{
    padding:10px 0;
    border-top:1px dashed #e1e1e1;
    color:#fc5a04;
    overflow:hidden;
}
.gm-comment-content .comment .answer p .atitle{
    color:#5a5a5a;
    font-weight:bold;
}
.gm-comment-content .comment .sarrow{
    position:absolute;
    top:30px;
    left:-6px;
    _left:-19px;
    width:6px;
    height:12px;
    line-height:10px;
    overflow:hidden;
    background:url(../images/icon_sarrow.gif) left top no-repeat;
}
.gm-comment-content .digg{
    padding-top:10px;
}
.gm-comment-content .digg .up img{
    padding:5px 5px 0 0;
    cursor:pointer;
}
.s-comment-form{
    border-radius:7px;
    background:#f1ffd5;
    margin:0 39px;
    padding-bottom:20px;
}
.s-comment-form .head-title{
    height:46px;
    background:url(../images/bg_comment_form_title.gif) 24px 0 no-repeat;
    border-bottom:1px solid #fff;
    text-indent:-9999px;
}
.s-comment-form .tip{
    width:762px;
    margin:10px auto;
}
.s-comment-form .form-items{
    width:762px;
    margin:10px auto;
}
.s-comment-form .form-items label.title{
    color:#83ab29;
    width:80px;
    padding-right:5px;
    text-align:right;
    display:inline-block;
}
.s-comment-form .form-items p{
    color:#83ab29;
    margin-bottom:5px;
}
.s-comment-form .form-items p .redstar{
    color:#b9102b;
    padding:0 3px;
}
.s-comment-form .form-items label.error{
    padding-left:5px;
    color:#b9102b;
}
.s-comment-form .form-items .tools-field{
    padding:10px 0 0 90px;
    height:25px;
}
.s-comment-form .form-items p input[type="text"], .s-comment-form .form-items p textarea{
    border:1px #7f9db9 solid;
}
.s-comment-form .form-items p input[type="text"]{
    width:270px;
    padding:2px 0;
}
.s-comment-form .form-items p input.fixed{
    margin-left:5px;
    border:none;
}
.s-comment-form .form-items p textarea{
    width:500px;
    height:74px;
}
.s-comment-form .form-items p input.submit, .s-comment-form .form-items p input.reset{
    width:51px;
    height:25px;
    border:none;
    cursor:pointer;
    margin-right:5px;
}
.s-comment-form .form-items p input.submit{
    background:url(../images/btn_comment_submit.gif) left top no-repeat;
}
.s-comment-form .form-items p input.reset{
    background:url(../images/btn_comment_reset.gif) left top no-repeat;
}


/* 点击改变数量 */
.num-input{
    overflow: hidden;
}
.num-input span{
    float: left;
}
.num-input .reduce,.num-input .plus{
    padding: 6px;
    width: 10px;
    height: 10px;
    cursor: pointer;
}
.num-input .reduce{
    background: url(../images/icon_nreduce.gif) center center no-repeat;
}
.num-input .plus{
    background: url(../images/icon_nplus.gif) center center no-repeat;
}
.num-input .ninput input{
    border: solid 1px #D4D4D4;
    padding: 1px;
    width: 30px;
    color: #888888;
    text-align: right;
}

.gm-tabs-head a{
    display: inline-block;
    text-decoration: none;
    color: #434242;
    padding: 0 16px;
    border: solid 1px #cfd1d4;
    border-bottom: none;
    background: #f2f2f3;
    margin-left: 2px;
    border-radius: 3px;
}
.gm-tabs .gm-tabs-head a{
    position: relative;
    top: 2px;
    margin-left: 2px;
}
.gm-tabs .gm-tabs-right-head a{
    margin-left: 0;
    margin-right: 2px;
}
.gm-tabs-head a:visited{
    color: #434242;
}
.gm-tabs-head a:hover,.gm-tabs-head a.current{
    text-decoration: underline;
    color: #1a1616;
    background: #f7f7f7;
}
.gm-tabs-head a:active{
    outline:none;
}

.gm-tabs-head a.current{
    text-decoration: none;
    cursor: default;
}
.gm-tabs-content{
    display: none;
}
.gm-tabs-content.current{
    display: block;
}
.gm-tabs .gm-tabs-content{
    padding: 10px;
    overflow: hidden;
}

