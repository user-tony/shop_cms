}
.gm-suser-info .p-order{
    background: url(../images/btn_order.gif) left top no-repeat;
    margin-right: 20px;
}
.gm-suser-info .p-cert{
    background: url(../images/btn_cert.gif) left top no-repeat;
}

.gm-suser-nav{
    padding: 10px 20px;
    background: #F4F4F4;
    line-height: 26px;
}
.gm-suser-nav .title{
    color: #6B9900;
    font-weight: bold;
    font-size: 14px;
}
.gm-suser-nav ul{
    margin-left: 30px;
}
.gm-suser-nav ul li{
    padding-left: 16px;
}
.gm-suser-nav ul li a,.gm-suser-nav ul li a:visited{
    color: #888;
}

/* simg preview */
.gm-ilink-prev{
    overflow: hidden;
}
.gm-ilink-prev .img{
    float:left;
    border: solid 1px #E6E6E6;
    margin-right: 10px;
}
.gm-ilink-prev .img img{
    width: 100px;
    height: 100px;
    display: block;
}
.gm-ilink-prev p{
    overflow: hidden;
}
.gm-ilink-prev .num{
