/* mod
全局模块样式
以 gm- 开头
************************************/

/* global pagination */
.gm-pagination{
    position: relative;
    overflow: hidden;
    line-height: 20px;
    padding: 10px;
    height: 26px;
}
.gm-pagination .pagination{
    overflow: hidden;
    float: left;
    padding-right: 10px;
}
.pagination span{
    float: left;
    padding-right: 4px;
}
.pagination a, .pagination span.current{
    display: block;
    text-align: center;
    border: solid 1px #94B547;
    min-width: 20px;
    margin-left: 2px;
    text-decoration: none;
    background: #fff;
    color: #94B547;
    outline: none;
}
.pagination a{
    padding: 0 2px;
}
.pagination span.current{
    padding-right: 0;
    margin-right: 4px;
}
.pagination span.gap{
    border: none;
    background: none;
    padding: 0 4px;
}
.pagination a:hover, .pagination span.current{
    background: #94B547;
    color: #fff;
    text-shadow:1px 0 0 #999;
}
.pagination span.page{
    width: 28px;
}

.pagination .first a, .pagination .last a, .pagination .prev a, .pagination .next a{
    padding: 0 6px;
}
.gm-pagination .disabled{
    color: #dad9d9;
}
.s-page-go{
    float: left;
    color: #94B547;
}
.s-page-go .input-page{
    background: #FFFFFF;
    color: #333;
    font-size: 12px;
    padding: 2px;
    border: 1px solid #DDDCDC;
    width: 30px;
    margin-left: 4px;
    margin-right: 4px;
}


/* global imgList */
.gm-img-list{
    position: relative;
    overflow: hidden;
}
.gm-img-list ul{
    position: relative;
    overflow: hidden;
}
.gm-img-list li{
    overflow: hidden;
    float: left;
    position: relative;
    padding: 5px;
    background: #fff;
    text-align: center;
    margin-bottom: 10px;
    width: 150px;
}
.gm-img-list li .simg img{
    display: block;
    padding: 5px;
    background: #fff;
    border: solid 1px #d2d2d2;
}
.gm-img-list a:hover .simg img{
    border: solid 1px #f3be6c;
    outline: solid 2px #ffe8c4;
}
.gm-img-list a{
    display: block;
    outline: none;
    text-decoration: none;
    color: #6d6d6d;
}
.gm-img-list a:hover{
    text-decoration: underline;
}


/* icon serach */
.gm-icon-search{
    height: 26px;
    float: left;
    overflow: hidden;
    border: solid 1px #DBDBDB;
    background: #fff;
}
.gm-icon-search .search-text{
    width: 350px;
    border: none;
    background: #fff;
    font-size: 12px;
    color: #A7A7A7;
    margin: 0;
    padding: 5px;
}
.gm-icon-search .search-submit{
    width: 32px;
    height: 26px;
    border: none;
    margin: 0;
    padding: 0;
    text-indent: -9999px;
    overflow: hidden;
    border-left: solid 1px #D6D6D6;
    background: url(../images/bg_mod_search_icon.gif) left top no-repeat;
    outline: none;
}

/* few tags list */
.gm-tags-list{
    color: #FF5D00;
}
.gm-tags-list a,.gm-tags-list a:visited{
    color: #6E6E6E;
    padding: 0 3px;
}

/* small icon list */
.gm-sicon-list ul{

}
.gm-sicon-list li{
    float: left;
    padding-left: 20px;
    position: relative;
    z-index: 100;
    zoom: 1;
}
.gm-sicon-list a, .gm-sicon-list a:visited{
    color: #7FB100;
    padding-left: 12px;
}
.gm-sicon-list .cart{
    background: url(../images/icon_user_cart.gif) left center no-repeat;
}
.gm-sicon-list .user{
    background: url(../images/icon_user.gif) left center no-repeat;
}


/* shoping cart */
.gm-cart-content{}
.gm-cart-content .top-tip{
    text-align: right;
}
.gm-cart-content .top-tip .cost{
    font-weight: bold;
    color: #FF6600;
    font-size: 14px;
}

.gm-cart-table{
    margin: 10px 0;
}
.gm-cart-table table{
    width: 100%;
}
.gm-cart-table table td{
    margin: 0;
    padding: 0;
    border: none;
    border-collapse : collapse;
}
.gm-cart-table-head, .gm-cart-table-foot{
    background: #EEFAD7;
    line-height: 40px;
    text-align: center;
    overflow: hidden;
}
.gm-cart-table-foot{
    height: 45px;
    overflow: visible;
}
.gm-cart-table-foot .top-tip .cost{
    font-size: 20px;
}

.gm-cart-table-foot .select_all{
    float: left;
    width: 100px;
    text-align: center;
}
.gm-cart-table .select_all input{
    position: relative;
    top: 2px;
    left: -2px;
}
.gm-cart-table-foot .delete_all{
    float: left;
    width: 100px;
    text-align: left;
}
.gm-cart-table-foot .top-tip{
    float: left;
    width: 450px;
    padding-right: 10px;
}
.gm-cart-table-foot .submit-field{
    float: right;
    overflow: hidden;
    clear:none;
}
.gm-cart-table-foot .submit-field .tip{
    float: left;
    width: 158px;
    height: 38px;
    overflow: hidden;
    margin-top: 3px;
    background: #fff url(../images/icon_gift.gif) 10px center no-repeat;
}
.gm-cart-table-foot .submit-field .tip input{
    position: relative;
    top: 2px;
    left: -2px;
}
.gm-cart-table-foot .submit-field .submit-form{
    display:block;
    width: 162px;
    height: 48px;
    overflow: hidden;
    border: none;
    margin: 0;
    padding: 0;
    position: relative;
    top: -3px;
    text-indent: -9999px;
    cursor: pointer;
}
.gm-cart-table-foot .submit-field .next-form{
    background: url(../images/btn_cart_next.gif) left top no-repeat;
}

.gm-cart-table-body{
    text-align: center;
    overflow: hidden;
    position: relative;
    zoom: 1;
}

.gm-cart-table-body .item{
    border-top: solid 2px #EEFAD7;
    overflow: hidden;
    position: relative;
}
.gm-cart-table-body .item td{
    padding: 10px 0;
    overflow: hidden;
    vertical-align: top;
}
.gm-cart-table-body .item .view-img{
    float: left;
    border: solid 1px #D7D7D7;
    margin-top: 4px;
}
.gm-cart-table-body .item .view-img img{
    display: block;
    width: 48px;
    height: 48px;
}
.gm-cart-table-body .item .title{
    float: left;
    width: 240px;
    padding-left: 6px;
    overflow: hidden;
    line-height: 22px;
    text-align: left;
}
.gm-cart-table-body .item .title a{
    display: block;
    overflow: hidden;
}
.gm-cart-table-body .item input.num{
    margin: 0;
    padding: 2px;
    border: solid 1px #D4D4D4;
    width: 30px;
    color: #888888;
    text-align: right;
}
.gm-cart-table-body .item .cost{
    font-weight: bold;
    color: #FF6600;
    font-size: 14px;
}
.gm-cart-table-body .item a{
    color: #7FB100;
    font-weight: bold;
}
.gm-cart-table-body .item a:visited{
    color: #7FB100;
}
.gm-cart-table-body .gm-sets-list{
    padding: 20px 0;
    margin-left: 100px;
    border-top: dotted 1px #E6E6E6;
}
.gm-cart-table-body .gm-sets-list ul{
    overflow:hidden;
}
.gm-cart-table-body .gm-sets-list ul li{
    float: left;
    padding:10px 5px;
    width: 140px;
}
.gm-cart-table-body .gm-sets-list ul li .title{
    width: 74px;
}
.gm-cart-table-body .gm-sets-list ul li .view-img{
    margin-top: 0;
}
.gm-cart-table-body .gm-sets-list ul li .view-img img{
    width: 58px;
    height: 58px;
}

.gm-cart-content .cert-title{
    clear: both;
    border-bottom: dotted 1px #DBDBDB;
    color: #6E6E6E;
    padding-left: 20px;
    background: url(../images/icon_cert_title.gif) 10px center no-repeat;
}



.gm-cart-table-body .item .comment{
    float: left;
    padding: 20px;
    text-align: left;
}
.gm-cart-table-body .item .comment textarea{
    width: 720px;
    height: 60px;
    border: solid 1px #7F9DB9;
    overflow: auto;
    padding: 4px;
}
.gm-cart-table-body .item .freight{
    float: left;
    width: 340px;
    padding: 20px;
    text-align: left;
}
.gm-cart-table-body .item .cost{
    float: left;
    padding: 20px 10px;
    font-size: 18px;
}
.gm-cart-table-body .item table .cost{
    float: none;
}
.gm-cart-table-foot .submit-field .back{
    float: left;
    width: 158px;
    height: 38px;
    overflow: hidden;
    margin-top: 3px;
    background: #fff;
}
.gm-cart-table-foot .submit-field .back a,.gm-cart-table-foot .submit-field .back a:visited{
    color: #888888;
    padding-left: 20px;
    background: url(../images/icon_blue_back.gif) left center no-repeat;
}
.gm-cart-table-foot .submit-field .order-form{
    background: url(../images/btn_cart_order.gif) left top no-repeat;
}

/* cert address */
.gm-address-list{
    margin: 20px;
    text-align: left;
}
.gm-address-list li{
    padding: 5px 0;
}
.gm-address-list li.select{
    font-size: 14px;
    color: #444;
    font-weight: bold;
}
.gm-address-list li input{
    position: relative;
    top: 2px;
    margin-right: 4px;
}
.gm-address-list li .set-default, .gm-address-list li .delete{
    font-size: 12px;
    color: #BBBBBB;
    padding-left: 20px;
    visibility : hidden;
    cursor: pointer;
}
.gm-address-list li .set-default:hover, .gm-address-list li .delete:hover{
    color: #A5CE00;
    text-decoration: underline;
}
.gm-address-list li.select .set-default,.gm-address-list li.select .delete{
    visibility : visible;
}
.gm-address-list select{
    border: solid 1px #7F9DB9;
    background: #fff;
    font-size: 12px;
}

.gm-address-list input.text{
    padding: 2px;
    border: solid 1px #7F9DB9;
    width: 360px;
    top: 0;
    font-size: 12px;
}
.gm-cart-content .set-address,.gm-cart-content .set-address:visited{
    color: #7FB100;
    font-weight: bold;
}
.gm-cart-content .m-address,.gm-cart-content .m-address:visited{
    float: right;
    color: #7FB100;
    font-weight: bold;
}
.gm-cart-content input.set-address{
    width: 150px;
    height: 44px;
    text-indent: -9999px;
    overflow: hidden;
    border: none;
    cursor: pointer;
    background: url(../images/btn_confirm.gif) left top no-repeat;
}

.gm-address-list .address-new{
    margin: 10px;
    margin-left: 20px;
}
.gm-address-list .address-new p{
    margin-bottom: 10px;
}
.gm-address-list strong,.gm-address-list a,.gm-address-list a:visited{
    color: #7FB100;
}

.gm-cart-table-foot .total-cost{
    padding: 20px 10px;
    font-size: 18px;
    color: #FF6600;
    font-weight: bold;
}

.gm-cart-content .cert-big-tip{
    padding: 20px;
    text-align: center;
    font-size: 14px;
    font-weight: bold;
}
.gm-cart-content .cert-big-tip .success{
    color: #329900;
    padding:20px 20px 20px 40px;
    background: url(../images/icon_right.gif) left center no-repeat;
}
.gm-cart-content .cert-big-tip .error{
    color: #FF4E00;
    padding:20px 20px 20px 40px;
    background: url(../images/icon_berror.gif) left center no-repeat;
}

/* post info */
.gm-propost-info{
    margin: 20px;
}
.gm-propost-info .tip{
    padding: 6px;
    background: #FFFFE5;
    border: solid 1px #FFCC7F;
}
.gm-propost-info .tip .w-link,.gm-propost-info .tip .w-link:visited{
    padding: 0 5px;
    color: #7FB100;
}
.gm-propost-info ul{
    margin: 10px;
}
.gm-propost-info ul .current{
    color: #C67700;
    font-weight: bold;
}
/* pay */
.gm-banks-wrap{
    text-align: left;
    margin-bottom: 20px;
