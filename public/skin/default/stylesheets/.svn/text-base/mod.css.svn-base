    position: relative;
}
.gm-banks-wrap .btitle{
    background: #EEFAD7;
    padding: 10px;
    font-size: 14px;
    font-weight: bold;
    margin: 10px 0;
}
.gm-banks-wrap .ilist{
    overflow: hidden;
}
.gm-banks-wrap .ilist li{
    padding: 10px;
    float: left;
    overflow: hidden;
    width: 150px;
    zoom: 1;
}
.gm-banks-wrap .ilist li input,.gm-banks-wrap .ilist li img{
    float: left;
}
.gm-banks-wrap .ilist li input{
    position: relative;
    top: 10px;
}
.gm-banks-wrap .ilist li img{
    padding-left: 5px;
}
.gm-banks-wrap .morebanks{
    clear: both;
    padding-left: 20px;
}

/* order list */
.gm-order-table{
    margin: 10px;
}
.gm-order-table table{
    width: 100%;
}
.gm-order-table table tr{
    border-bottom: dotted 1px #E4E4E4;
}
.gm-order-table table thead tr{
    border-bottom: solid 1px #E4E4E4;
}
.gm-order-table table th,.gm-order-table table td{
    padding: 5px 0;
    text-align: center;
}
.gm-order-table table th{
    color: #FF7000;
}
.gm-order-table table a,.gm-order-table table a:visited{
    color: #A5CE00;
    font-weight: bold;
}
.gm-order-table table td .ready{
    color: #FF0000;
}
.gm-order-table table td .finished,.gm-order-table table td .vip{
    color: #309B00;
}
.gm-order-table table td .finished-check{
    color: #309B00;
}
.gm-order-table table td .cancel{
    color: #888888;
    padding: 4px;
    border: solid 1px #E4E4E4;
    background: #F5F5F5;
    font-weight: normal;
}
.gm-order-table table td .cancel:hover{
    text-decoration: none;
    color: #A5CE00;
}
.gm-order-table .total-cost{
    padding: 10px 20px;
    text-align: right;
}
.gm-order-table .total-cost strong{
    color: #f00;
}

.gm-cart-content .order-title{
    font-size: 14px;
    line-height: 32px;
    margin: 10px;
    overflow: hidden;
}
.gm-cart-content .order-title p{
    float: left;
}
.gm-cart-content .order-title .num{
    width: 450px;
}
.gm-cart-content .order-title .num strong{
    color: #A5CE00;
}
.gm-cart-content .order-title .state{
    width: 280px;
}
.gm-cart-content .order-title .state strong{
    color: #A5CE00;
}
.gm-cart-content .order-title .pay{
    overflow: hidden;
    float: right;
}
.gm-cart-content .order-title .pay a{
    float: left;
    width: 94px;
    height: 36px;
    overflow: hidden;
    text-indent: -9999px;
    background: url(../images/btn_cpay.gif) left top no-repeat;
}

.gm-order-overview{
    border: solid 1px #DBDBDB;
}
.gm-order-overview-title{
    padding: 5px;
    position: relative;
    overflow: hidden;
}
.gm-order-overview-title .order-id{
    float: left;
    font-weight: bold;
    width: 300px;
}
.gm-order-overview-title .order-id a,.gm-order-overview-title .order-id a:visited{
    color: #888888;
}
.gm-order-overview-title .order-id a:hover{
    color: #7FB100;
}
.gm-order-overview-title .order-time{
    float: left;
}
.gm-order-overview-title .order-state{
    float: right;
}
.gm-order-overview-title .order-state .green{
    color: #309B00;
}
.gm-order-overview-title .order-state .red{
    color: #FF0000;
}
